<div id="HelloWorld_${instanceId}" class="super-widget wcm-widget-class fluig-style-guide"
     data-params="HelloWorld.instance({message: 'Hello world'})">

    
    <h2>Consulta CEP:</h2>

    <div class="row">
      
      <div class=" col-md-2">
		    <input name="cep" id="cep" class="form-control" type="text" placeholder="insert cep">
	    </div>
      <div class="col-md-3">
      		<button class="btn btn-default" id="btn-search" data-search >Search</button>
      </div>
      </div>
      <div class="row">
      <div class="col-md-6">
        <label>Street:</label>
        <input name="street" id="street" type="text" class="form-control" />
      </div>
      <div class="col-md-2">
        <label>Neighborhood:</label>
        <input name="neighborhood" id="neighborhood" type="text" class="form-control" />
      </div>
      <div class="col-md-4">
        <label>City:</label>
        <input name="city" id="city" type="text" class="form-control" />
      </div>
    </div>

  
</div>
